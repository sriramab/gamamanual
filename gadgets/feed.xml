<Module>
<ModulePrefs title="RSS Feed Control" height="400"/>
<UserPref name="feedurl" display_name="Feed" default_value=""/>
<Content type="html">
<![CDATA[
<script type="text/javascript" src="gadgets/spin.js"></script>
<div id="refresh" style="padding:10px;font-size: small;" ><button onclick="javascript:initialize()">Refresh</button></div>
<div style="padding:10px"> <div id="feed" style="font-size: small;"></div> </div>
<style type=text/css>
a:link {font-weight: ; color:;}
a:hover {color: ;}
.gfc-resultsHeader {display:none;}
.gfc-results {
  padding-top: 11px;
}
.gfc-result {padding-top:3px; margin:0px; font: 10pt verdana;}
.gf-result.gf-author,
.gf-result.gf-spacer,
.gf-result.gf-relativePublishedDate
.gf-result.gf-snippet
.gf-result.gf-title {
        font: 10pt verdana;
}
.gfc-tabsArea {padding-bottom: 4px; font: 11pt verdana; font-weight: bold;}
.gfc-tabHeader {margin:1px; padding-top: 2px; padding-bottom: 2px; border-top-color:;}
.gfc-tabHeader.gfc-tabhInactive {color: white; background-color: #42618F;}
.gfc-tabHeader.gfc-tabhInactive:hover {color: #950041; border-top-
color: #950041; background-color: white;}
.gfc-tabHeader.gfc-tabhActive:hover {color: #950041; border-top-color:
#950041; background-color: white;}
a.gf-title {padding:0px 0px}
 </style>
<script type="text/javascript" src="https://www.google.com/jsapi"></script>
<script type="text/javascript">
  var target = document.getElementById('feed');
  var spinner = new Spinner().spin(target);
  google.load("feeds", "1");
  function initialize() {
    var feedControl = new google.feeds.FeedControl();
    feedControl.addFeed("https://code.google.com/feeds/p/gama-platform/svnchanges/basic?path=/branches/"+"&t="+new Date().getTime(), "Revisions");
    feedControl.addFeed("https://code.google.com/feeds/p/gama-platform/issueupdates/basic"+"&t="+new Date().getTime(), "Issues");
    feedControl.addFeed("https://groups.google.com/forum/feed/gama-platform/msgs/atom.xml?num=15","Users forum");
    feedControl.addFeed("https://groups.google.com/forum/feed/gama-dev/msgs/atom.xml?num=15","Devs forum");
    feedControl.addFeed("https://code.google.com/feeds/p/gama-platform/svnchanges/basic?path=/wiki/"+"&t="+new Date().getTime(), "Wiki");
    feedControl.setNumEntries(250);
    feedControl.setLinkTarget(google.feeds.LINK_TARGET_BLANK);
    feedControl.draw(document.getElementById("feed"), {drawMode : google.feeds.FeedControl.DRAW_MODE_TABBED});
    spinner.stop();
 }
 google.setOnLoadCallback(initialize);
</script>
]]>
